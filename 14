#include <iostream>
#include <forward_list>

int main()
{
	std::forward_list <int> numbers = { 1, 2, 3, 4, 5 };

	int first = numbers.front(); // 1
	std::cout << first << "\n\n";
	for (int n : numbers)
		std::cout << n << "\t";
	std::cout << std::endl;
	return 0;
}



/*Вывод:
1

1       2       3       4       5*/
