#include <iostream>
#include <list>

using namespace std;

int main()
{
	list<int>numbers = { 1, 2, 3, 4, 5 };
	numbers.push_back(23);	// { 1, 2, 3, 4, 23 }
	numbers.push_front(15); // { 15, 1, 2, 3, 4, 5, 23 }
	numbers.emplace_back(24);  // { 15, 1, 2, 3, 4, 5, 23, 24}
	numbers.emplace_front(14);	// { 14, 15, 1, 2, 3, 4, 5, 23, 24}
	for (int n : numbers)
		cout << n << "\t";
	cout << endl;

	return 0;
}

/*
Вывод:
14      15      1       2       3       4       5       23      24 */
