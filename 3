#include <iostream> 
#include <vector>

using namespace std;
int main() {
	setlocale(0, "");

	vector <int> vec_string(2); //// добавлен тип параметров вектора ////

	vec_string[0] = 2;
	vec_string[1] = 3;

	for (int i = 0; i < vec_string.size(); i++) {
		cout << vec_string[i] << " ";
	}

	cout << endl;
	vec_string.insert(vec_string.begin(), 1); // добавили элемент в начало

	for (int i = 0; i < vec_string.size(); i++) {
		cout << vec_string[i] << " ";
	}

	cout << endl;
	vec_string.insert(vec_string.end(), 4); // добавили элемент в конец

	for (int i = 0; i < vec_string.size(); i++) {
		cout << vec_string[i] << " ";
	}
	system("pause");
	return 0;
}

/*Вывод:
2 3
1 2 3
1 2 3 4 */
